	</target>
	<target name="assemble.org.eclipse.mylyn.monitor_feature">
		<ant antfile="${assembleScriptName}" dir="${buildDirectory}" />
	</target>
	<target name="assemble.org.eclipse.mylyn.sandbox.ui_feature">
