    <setupTask
        xsi:type="pde:APIBaselineFromTargetTask"
        id="mylyn.api.baseline"
        predecessor="mylyn.target.api"
        name="Mylyn Baseline"
        targetName="Mylyn Modular Target for API"/>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask"
        id="mylyn.workingsets">
      <workingSet
          name="Mylyn">
        <predicate
            xsi:type="predicates:OrPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.mylyn-aggregator"
              relativePathPattern="org.eclipse.mylyn.*"/>
          <operand
              xsi:type="predicates:NamePredicate"
              pattern=".*[-.]parent|.*-aggregator"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Commons">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.commons/.*"/>
      </workingSet>
      <workingSet
          name="Tasks">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.mylyn-aggregator"
              relativePathPattern="mylyn.tasks/org.eclipse.mylyn.*"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Bugzilla Connector">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.tasks/connectors/bugzilla/.*"/>
      </workingSet>
      <workingSet
          name="Bugzilla Rest Connector">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.tasks/connectors/bugzilla-rest/.*"/>
      </workingSet>
      <workingSet
          name="Gitlab Connector">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.tasks/connectors/gitlab/.*"/>
      </workingSet>
      <workingSet
          name="Context">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.mylyn-aggregator"
              relativePathPattern="mylyn.context/.*"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Builds">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.builds/.*"/>
      </workingSet>
      <workingSet
          name="Reviews">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.reviews/.*"/>
