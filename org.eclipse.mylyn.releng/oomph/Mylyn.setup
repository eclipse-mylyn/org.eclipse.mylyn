  <project name="mylyn"
      label="Mylyn">
    <setupTask
        xsi:type="git:GitCloneTask"
        id="git.clone.mylyn"
        remoteURI="${mylyn-github-orga}/org.eclipse.mylyn">
      <annotation
          source="http://www.eclipse.org/oomph/setup/InducedChoices">
        <detail
            key="inherit">
          <value>github.remoteURIs</value>
        </detail>
        <detail
            key="label">
          <value>Mylyn GitHub Repository</value>
        </detail>
        <detail
            key="target">
          <value>remoteURI</value>
        </detail>
      </annotation>
      <configSections
          name="core">
        <properties
            key="autocrlf"
            value="false"/>
      </configSections>
      <description>Mylyn without Docs</description>
    </setupTask>
    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Doomph.redirection.mylyn"
        value="=https://raw.githubusercontent.com/eclipse-mylyn/org.eclipse.mylyn/main/org.eclipse.mylyn.releng/oomph/Mylyn.setup->${git.clone.mylyn.location|uri}/org.eclipse.mylyn.releng/oomph/Mylyn.setup"
        vm="true"/>
    <setupTask
        xsi:type="projects:ProjectsImportTask">
      <sourceLocator
          rootFolder="${git.clone.mylyn.location}"
          locateNestedProjects="true">
        <excludedPath>mylyn.builds/org.eclipse.mylyn.hudson.tests/testdata</excludedPath>
        <excludedPath>mylyn.reviews/tbr</excludedPath>
        <excludedPath>mylyn.docs</excludedPath>
        <excludedPath>org.eclipse.mylar.tests</excludedPath>
        <excludedPath>mylyn.versions/org.eclipse.mylyn.versions.tests/target/work/data</excludedPath>
        <excludedPath>mylyn.context/org.eclipse.mylyn.java.tests/target/work/data</excludedPath>
        <excludedPath>mylyn.tasks/org.eclipse.mylyn.tasks.tests/target</excludedPath>
      </sourceLocator>
      <description>Import Releng Project</description>
    </setupTask>
    <setupTask
        xsi:type="pde:TargetPlatformTask"
        predecessor="mylyn.workingsets"
        name="mylyn-e4.28">
      <description>Active Mylyn Target Platform</description>
    </setupTask>
    <setupTask
        xsi:type="setup.targlets:TargletTask"
        id="mylyn.target.api"
        predecessor="git.clone.mylyn //@projects[name='mylyn']/@setupTasks.2 //@projects[name='mylyn']/@setupTasks.3"
        targetName="Mylyn Modular Target for API"
        activateTarget="false">
      <targlet
          name="Mylyn"
          includeSources="false">
        <requirement
            name="org.eclipse.mylyn_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.context_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.trac_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.discovery.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.ide_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.java_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.pde_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.team_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.bugzilla_feature.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.git.feature.group"/>
        <requirement
            name="org.eclipse.cdt.mylyn.feature.group"/>
        <requirement
            name="org.eclipse.mylyn.github.feature.feature.group"/>
        <repositoryList
            name="latest">
          <repository
              url="https://download.eclipse.org/mylyn/releases/3.26"/>
          <repository
              url="https://download.eclipse.org/egit/updates-6.2"/>
          <repository
              url="https://download.eclipse.org/tools/cdt/releases/11.0"/>
          <repository
              url="https://download.eclipse.org/eclipse/updates/4.26/R-4.26-202211231800/"/>
        </repositoryList>
      </targlet>
    </setupTask>
    <setupTask
        xsi:type="pde:APIBaselineFromTargetTask"
        id="mylyn.api.baseline"
        predecessor="mylyn.target.api"
        name="Mylyn Baseline"
        targetName="Mylyn Modular Target for API"/>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask"
        id="mylyn.workingsets">
      <workingSet
          name="Mylyn">
        <predicate
            xsi:type="predicates:OrPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.mylyn-aggregator"
              relativePathPattern="org.eclipse.mylyn.*"/>
          <operand
              xsi:type="predicates:NamePredicate"
              pattern=".*[-.]parent|.*-aggregator"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Commons">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.commons/.*"/>
      </workingSet>
      <workingSet
          name="Tasks">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.mylyn-aggregator"
              relativePathPattern="mylyn.tasks/org.eclipse.mylyn.*"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Trac Connector">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.tasks/connectors/trac/.*"/>
      </workingSet>
      <workingSet
          name="Bugzilla Connector">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.tasks/connectors/bugzilla/.*"/>
      </workingSet>
      <workingSet
          name="Bugzilla Rest Connector">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.tasks/connectors/bugzilla-rest/.*"/>
      </workingSet>
      <workingSet
          name="Context">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.mylyn-aggregator"
              relativePathPattern="mylyn.context/.*"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Builds">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.builds/.*"/>
      </workingSet>
      <workingSet
          name="Reviews">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.reviews/.*"/>
      </workingSet>
      <workingSet
          name="Versions">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.versions/.*"/>
      </workingSet>
      <workingSet
          name="GitHub">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.github/.*"/>
      </workingSet>
      <workingSet
          name="EGit">
        <predicate
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.mylyn-aggregator"
            relativePathPattern="mylyn.egit/.*"/>
      </workingSet>
    </setupTask>
    <stream
        name="main"
        label="main"/>
    <description>The new Setup for Mylyn hosted on GitHub</description>
  </project>
