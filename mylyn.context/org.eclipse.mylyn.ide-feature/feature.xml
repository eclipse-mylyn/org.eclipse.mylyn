<feature
      id="org.eclipse.mylyn.ide.feature"
      label="%featureName"
      version="4.0.0.qualifier"
      provider-name="%providerName"
      plugin="org.eclipse.mylyn.context.core"
      license-feature="org.eclipse.license"
      license-feature-version="2.0.2">

   <description url="http://eclipse.org/mylyn">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.mylyn.context.feature" version="4.0.0" match="equivalent"/>
      <import feature="org.eclipse.mylyn.team.feature" version="4.0.0" match="equivalent"/>
      <import feature="org.eclipse.mylyn.tasks.feature" version="4.0.0" match="equivalent"/>
   </requires>

   <plugin
         id="org.eclipse.mylyn.ide.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
